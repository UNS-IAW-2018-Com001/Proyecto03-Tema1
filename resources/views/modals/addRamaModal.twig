<!-- Add Grupo Modal HTML -->
	<div id="addModal" class="modal fade">
		<div class="modal-dialog">
			<div class="modal-content" id="contentadd">
				<form>
					<div class="modal-header">						
						<h4 class="modal-title">Añadir Rama</h4>
						<button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
					</div>
					<div class="modal-body">					
						<div class="form-group">
							<label>Nombre <label class="obligatorio">(*)</label></label>
							<input type="text" class="form-control" required id='addNombreRama'>
						</div>
						<div class="form-group">
							<label>Edad Minima <label class="obligatorio">(*)</label></label>
							<!input type="text" class="form-control" required id='addEdadMinima'>
							<select class="selectpicker form-control col-md-10" id="addEdadMinima" data-size="5">
							{% for i in 4..21 %}
								<option>{{i}}</option>
							{% endfor %}
							</select>
						</div>
						<div class="form-group">
							<label>Edad Maxima <label class="obligatorio">(*)</label></label>
							<!input type="text" class="form-control" required id='addEdadMaxima'>
							<select class="selectpicker form-control col-md-10" id="addEdadMaxima" data-size="5">
							{% for i in 4..21 %}
								<option>{{i}}</option>
							{% endfor %}
							</select>
						</div>
						<div class="form-group">
							<label>Fecha de Inicio de Inscripcion <label class="obligatorio">(*)</label></label>
							<input type="text" class="form-control dateFormat" required readonly id='addFechaInicio'>
						</div>
						<div class="form-group">
							<label>Fecha de Fin de Inscripcion <label class="obligatorio">(*)</label></label>
							<input type="text" class="form-control dateFormat" required readonly id='addFechaFin'>
						</div>
						<div class="form-group">
							<label>Tipo <label class="obligatorio">(*)</label></label>
							<select class="selectpicker form-control col-md-10" id='addTipo'>
  								<option>Rama Femenina</option>
  								<option>Rama Masculina</option>
  								<option>Rama Mixta</option>
							</select>

						</div>
											
					</div>	
					</div>
					<div class="modal-footer">
						<input type="button" class="btn btn-default" data-dismiss="modal" value="Cancelar">
						<input type="submit" class="btn btn-success" value="Añadir" id="addRamaSubmit">
					</div>
				</form>
			</div>
		</div>
	</div>